{
    "name": "media-archive",
    "version": "0.0.8a",
    "description": "Web app for storing and indexing digitized documents, photos, audio, and video.",
    "private": true,
    "author": "Jonathan Schoonhoven",
    "license": "MIT",
    "homepage": "https://github.com/jtschoonhoven/media-archive#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/jtschoonhoven/media-archive.git"
    },
    "scripts": {
        "analyze": "npx source-map-explorer dist/bundle.client.*",
        "clean": "find ./dist/* ! -name 'favicon.ico' | xargs rm",
        "build-dev": "npm run clean && npx webpack --mode=development --config=config/webpack.config.js",
        "watch": "npm run clean && npx webpack --watch --mode=development --config=config/webpack.config.js",
        "build": "npm run clean && npx webpack --mode=production --config=config/webpack.config.js",
        "start-dev": "npx nodemon --inspect server/server.js",
        "start": "node server/server.js",
        "zip": "npm run build && zip -r dist/archive.zip ./dist ./server ./config package.json",
        "deploy": "npm run zip && eb deploy --verbose",
        "db": "pg_ctl -D ./.pgdb start"
    },
    "dependencies": {
        "@godaddy/terminus": "^4.1.0",
        "body-parser": "^1.18.3",
        "bootstrap": "^4.1.3",
        "config": "^3.0.0",
        "cookie-session": "^2.0.0-beta.3",
        "ejs": "^2.6.1",
        "express": "^4.16.4",
        "express-session": "^1.15.6",
        "express-winston": "^3.0.1",
        "form-serialize": "^0.7.2",
        "formik": "^1.3.2",
        "helmet": "^3.15.0",
        "immutable": "^4.0.0-rc.12",
        "lodash": "^4.17.11",
        "passport": "^0.4.0",
        "passport-google-oauth20": "^1.0.0",
        "pg": "^7.7.1",
        "popper.js": "^1.14.5",
        "prop-types": "^15.6.2",
        "query-string": "^5.1.1",
        "react": "^16.6.3",
        "react-dom": "^16.6.3",
        "react-redux": "^5.1.1",
        "react-router": "^4.3.1",
        "react-router-dom": "^4.3.1",
        "redux": "^4.0.1",
        "sql-template-strings": "^2.2.2",
        "winston": "^3.1.0"
    },
    "devDependencies": {
        "@babel/core": "^7.1.6",
        "@babel/preset-env": "^7.1.6",
        "@babel/preset-react": "^7.0.0",
        "autoprefixer": "^9.3.1",
        "babel-eslint": "^10.0.1",
        "babel-loader": "^8.0.4",
        "css-loader": "^1.0.1",
        "eslint": "^5.9.0",
        "eslint-config-airbnb-base": "^13.1.0",
        "eslint-loader": "^2.1.1",
        "eslint-plugin-import": "^2.14.0",
        "eslint-plugin-react": "^7.11.1",
        "exports-loader": "^0.7.0",
        "ignore-loader": "^0.1.2",
        "mini-css-extract-plugin": "^0.4.5",
        "node-sass": "^4.10.0",
        "nodemon": "^1.18.7",
        "optimize-css-assets-webpack-plugin": "^5.0.1",
        "postcss-loader": "^3.0.0",
        "precss": "^3.1.2",
        "sass-loader": "^7.1.0",
        "serve": "^10.1.1",
        "uglifyjs-webpack-plugin": "^2.0.1",
        "webpack": "^4.26.1",
        "webpack-cli": "^3.1.2",
        "webpack-merge": "^4.1.4"
    }
}
