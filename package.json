{
    "name": "media-archive",
    "version": "0.0.9a",
    "description": "Web app for storing and indexing digitized documents, photos, audio, and video.",
    "private": true,
    "author": "Jonathan Schoonhoven",
    "license": "MIT",
    "homepage": "https://github.com/jtschoonhoven/media-archive#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/jtschoonhoven/media-archive.git"
    },
    "scripts": {
        "analyze": "npx source-map-explorer dist/bundle.client.*",
        "clean": "find ./dist/* -type f  ! -name 'favicon.ico' ! -path './dist/assets/*' | xargs rm",
        "lint": "npx eslint --ext '.js,.jsx,.ts,.tsx' 'client/' 'server/'",
        "build": "npm run clean && npx webpack --mode=production --config=config/webpack.config.js",
        "build:dev": "npm run clean && npx webpack --mode=development --config=config/webpack.config.js",
        "watch": "npm run clean && npx webpack --watch --mode=development --config=config/webpack.config.js",
        "start": "node server/server.js",
        "start:dev": "npx nodemon --inspect server/server.js --watch ./server --watch ./dist/bundle.server.js",
        "debug": "npx node-inspect server/server.js",
        "zip": "npm run build && zip -r dist/archive.zip ./dist ./server ./config package.json",
        "deploy": "npm run zip && eb deploy --verbose",
        "db": "pg_ctl -D ./.pgdb start"
    },
    "dependencies": {
        "@babel/polyfill": "~7.2.5",
        "@godaddy/terminus": "~4.1.0",
        "aws-sdk": "~2.382.0",
        "body-parser": "~1.18.3",
        "bootstrap": "~4.3.1",
        "config": "^1.30.0",
        "cookie-session": "~2.0.0-beta.3",
        "csv-parse": "~4.4.3",
        "csv-stringify": "~5.3.0",
        "deepcopy": "~1.0.0",
        "ejs": "~2.6.1",
        "es6-promise-pool": "~2.5.0",
        "express": "~4.16.4",
        "express-session": "~1.15.6",
        "express-winston": "~3.0.1",
        "formik": "~2.1.0",
        "helmet": "~3.15.0",
        "history": "~4.7.2",
        "joi": "~14.3.1",
        "jquery": "^3.4.1",
        "lodash": "~4.17.11",
        "passport": "~0.4.0",
        "passport-google-oauth20": "~1.0.0",
        "pdf-parse": "^1.1.1",
        "pg": "~7.7.1",
        "popper.js": "~1.14.5",
        "prop-types": "~15.6.2",
        "query-string": "~5.1.1",
        "react": "~16.8.6",
        "react-bootstrap": "~1.0.0-beta.9",
        "react-dom": "~16.8.6",
        "react-redux": "~5.1.1",
        "react-router": "~4.3.1",
        "react-router-dom": "~4.3.1",
        "react-show-more": "^2.0.0",
        "react-truncate": "^2.4.0",
        "redux": "~4.0.1",
        "sql-template-strings": "~2.2.2",
        "stream-transform": "~2.0.1",
        "url-join": "~4.0.0",
        "uuid": "~3.3.2",
        "winston": "~3.1.0"
    },
    "devDependencies": {
        "@babel/core": "^7.1.6",
        "@babel/preset-env": "~7.1.6",
        "@babel/preset-react": "~7.0.0",
        "@types/react": "~16.7.18",
        "@types/react-dom": "~16.0.11",
        "@types/react-redux": "~6.0.12",
        "@types/react-router-dom": "~4.3.1",
        "@types/url-join": "~4.0.0",
        "@typescript-eslint/eslint-plugin": "~1.4.2",
        "@typescript-eslint/parser": "~1.4.2",
        "acorn": "^7.1.0",
        "autoprefixer": "~9.3.1",
        "babel-eslint": "~10.0.1",
        "babel-loader": "~8.0.4",
        "config-webpack": "~1.0.4",
        "css-loader": "~1.0.1",
        "eslint": "^5.3.0",
        "eslint-config-airbnb": "~17.1.0",
        "eslint-import-resolver-typescript": "~1.1.1",
        "eslint-plugin-import": "^2.20.0",
        "eslint-plugin-jsx-a11y": "~6.2.1",
        "eslint-plugin-react": "^7.17.0",
        "exports-loader": "~0.7.0",
        "ignore-loader": "~0.1.2",
        "mini-css-extract-plugin": "~0.4.5",
        "node-inspect": "~1.11.5",
        "node-sass": "^4.13.1",
        "nodemon": "~1.18.7",
        "optimize-css-assets-webpack-plugin": "~5.0.1",
        "postcss-loader": "~3.0.0",
        "precss": "~3.1.2",
        "sass-loader": "~7.1.0",
        "serve": "~10.1.1",
